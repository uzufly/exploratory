{
  "name": "@uzufly/custom-element-element",
  "version": "0.1.0",
  "description": "Boilerplate for creating a ‹custom-element› Web Component",
  "keywords": [
    "custom-element",
    "web-components",
    "javascript",
    "lit"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "type": "module",
  "module": "dist/custom-element.js",
  "source": "src/custom-element.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uzufly/elements.git",
    "directory": "custom-element-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/uzufly/elements/issues"
  },
  "homepage": "https://github.com/uzufly/elements#readme",
  "scripts": {
    "clean": "npx del --force dist .parcel-cache",
    "format": "prettier \"{src,examples}/**/*.{js,html,css}\" --write",
    "lint": "eslint . --ext .js",
    "start": "parcel index.html",
    "build": "parcel build --public-url ./",
    "prebuild": "npm run clean && npm run lint && npx make-dir dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "del-cli": "^5.0.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-custom-elements": "^0.0.7",
    "make-dir-cli": "^3.0.0",
    "parcel": "latest",
    "prettier": "^2.8.6"
  },
  "dependencies": {
    "lit": "^2.7.0"
  },
  "browser": {
    "assert": false,
    "buffer": false,
    "crypto": false,
    "fs": false,
    "http": false,
    "https": false,
    "path": false,
    "process": false,
    "querystring": false,
    "stream": false,
    "url": false,
    "util": false,
    "zlib": false
  }
}
